{
  "name": "geo-tz-fast",
  "version": "0.0.0-semantically-released",
  "description": "A node.js module to find the timezone at specific gps coordinates",
  "keywords": [
    "timezone"
  ],
  "author": [
    "Evan Siroky",
    "Arne Seib <arne.seib@windy.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/windycom/node-geo-tz.git"
  },
  "bugs": {
    "url": "https://github.com/windycom/node-geo-tz/issues"
  },
  "homepage": "https://github.com/windycom/node-geo-tz#readme",
  "engines": {
    "node": ">=7"
  },
  "main": "index.js",
  "scripts": {
    "codeclimate": "npm run cover && npm run codeclimate-send",
    "codeclimate-send": "./node_modules/.bin/codeclimate-test-reporter < coverage/lcov.info",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- tests/**/*.js",
    "prepublish": "node scripts/unzip-data.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha \"tests/**/*.js\"",
    "updateTzData": "node scripts/update-data.js"
  },
  "devDependencies": {
    "async": "^2.1.5",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "follow-redirects": "^1.2.1",
    "fs-extra": "^4.0.3",
    "istanbul": "^0.4.2",
    "jsts": "^1.3.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.0",
    "nock": "^9.0.9",
    "rimraf": "^2.6.1",
    "semantic-release": "^8.0.0",
    "yauzl": "^2.6.0",
    "yazl": "^2.4.2"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.3",
    "geobuf": "^3.0.0",
    "moment-timezone": "^0.5.15",
    "pbf": "^3.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
